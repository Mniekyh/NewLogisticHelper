@{
    ViewData["Title"] = "Bindings";
    <link href="~/styles/style.css" rel="stylesheet" />
}
<div class="agile-right">
    <a>@Html.ActionLink("Dodaj", "Create", "Account")</a>
</div><div class="agile-right">
    <a>@Html.ActionLink("gogle", "Index", "GoogleMap")</a>
</div>
<div class="agile-center">
    <ol>
        <li>Cześć, Twoje ID to: @TempData.Peek("userId")</</li>
        <li>Lista Twoich adresów: </li>
    </ol>
</div>
<div class="agile-center">
    <table>
        <tbody>
            @TempData.Peek("addressesList")
            @{
                @foreach (var address in (dynamic) TempData["addresseslist"])
                {
                    <tr>
                        <td style="white-space:nowrap">>@address[0], @address[1]</td>
                        <td>
                            <button type="button" class="btn">
                                @Html.ActionLink(
                                    "Szczegóły",
                                    "Details",
                                    "Account",
                                    routeValues: new {
                                        miejscowosc = @address[0],
                                        ulica = @address[1]
                                    },
                                    htmlAttributes: null)
                                </button>
                        </td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>