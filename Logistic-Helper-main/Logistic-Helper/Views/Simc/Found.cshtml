@{
    ViewData["Title"] = "Search";
}
<div id = "koza" style = "width:100px; height:100px;">
    </div>
<link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/start/jquery-ui.css">


        @foreach(var obj in Model)
        {
         
                <p> Województwo:  @obj.Wojewodztwo
           

                     Powiat:  @obj.Powiat
            
                
                    Gmina: @obj.Gmina
                    Miejsowość: @obj.Nazwa
                    <div class = "w-75 btn-group" role = "group">
                        <a asp-controller = "Simc" asp-action ="Search" asp-route-id="@obj.Symbol" class = "btn btn-primary">
                            <i class="bi bi-pen"></i> Wybierz</a>
                    </p>
        }
