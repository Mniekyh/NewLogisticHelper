@{
    ViewData["Title"] = "Search";
}
<div id = "koza" style = "width:100px; height:100px;">
    </div>
<link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/start/jquery-ui.css">


        @foreach(var obj in Model)
        {
         
                <p> Województwo:  @obj.Wojewodztwo
           

                     Powiat:  @obj.Powiat
            
                
                    Gmina: @obj.Gmina
                    Miejsowość: @obj.Nazwa

                    @*Button which will allow user choose village for showing on map, now just create this shit....

                    God Im awesome...
                    *@
                    <div class = "w-75 btn-group" role = "group">
                        <a asp-controller = "Simc" asp-action ="Details"  asp-route-symbol="@obj.Symbol" asp-route-wojewodztwo = "@obj.Wojewodztwo" class = "btn btn-primary">
                            <i class="bi bi-pen"></i> Wybierz</a>
                    </div>

                    <div class = "w-75 btn-group" role = "group">
                        <a href = 'https://www.google.com/maps/search/+"@obj.Wojewodztwo"+"@obj.Powiat"+"@obj.Nazwa"'>
                            <i class="bi bi-pen"></i> Google</a>
                    </div>
                    </p>
        }


       
